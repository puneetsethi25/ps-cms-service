{
  "kind": "collectionType",
  "collectionName": "care_homes",
  "info": {
    "singularName": "care-home",
    "pluralName": "care-homes",
    "displayName": "care_home",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "about": {
      "type": "text"
    },
    "missionStatement": {
      "type": "richtext"
    },
    "procedures": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive",
        "Blocked"
      ],
      "default": "Active",
      "required": true
    },
    "isDeleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "amenities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::amnesty.amnesty"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Care Facility",
        "Care Agency"
      ],
      "required": true
    },
    "medicalSpecialization": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::medical-specialization.medical-specialization",
      "inversedBy": "care_homes"
    },
    "gender_preferences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::gender-preference.gender-preference"
    },
    "care_workers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::care-worker.care-worker"
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::care-home-address.care-home-address"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "suites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::suite.suite",
      "mappedBy": "care_home"
    },
    "lifeStyleOptions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lifestyle-option.lifestyle-option",
      "inversedBy": "care_homes"
    },
    "cost": {
      "type": "integer"
    },
    "ourVillage": {
      "type": "text"
    },
    "villageLife": {
      "type": "text"
    },
    "care_home_address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::care-home-address.care-home-address",
      "mappedBy": "careHome"
    },
    "care_managers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::care-manger.care-manger",
      "mappedBy": "care_home"
    }
  }
}
