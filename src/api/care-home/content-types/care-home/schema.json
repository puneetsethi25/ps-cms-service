{
  "kind": "collectionType",
  "collectionName": "care_homes",
  "info": {
    "singularName": "care-home",
    "pluralName": "care-homes",
    "displayName": "care_home",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "about": {
      "type": "string"
    },
    "mission_statement": {
      "type": "richtext"
    },
    "procedures": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive",
        "Blocked"
      ],
      "default": "Active",
      "required": true
    },
    "is_deleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "phone_number": {
      "type": "string",
      "required": true
    },
    "amenities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::amnesty.amnesty"
    },
    "provider_type": {
      "type": "enumeration",
      "enum": [
        "CareFacility",
        "CareAgency"
      ],
      "required": true
    },
    "medical_specializations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medical-specialization.medical-specialization"
    },
    "gender_preferences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::gender-preference.gender-preference"
    },
    "lifestyle_options": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lifestyle-option.lifestyle-option",
      "mappedBy": "care_homes"
    },
    "care_managers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::care-manger.care-manger"
    },
    "care_workers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::care-worker.care-worker"
    }
  }
}
