{
  "kind": "collectionType",
  "collectionName": "care_homes",
  "info": {
    "singularName": "care-home",
    "pluralName": "care-homes",
    "displayName": "care_home",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "about": {
      "type": "string"
    },
    "mission_statement": {
      "type": "richtext"
    },
    "procedures": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive",
        "Blocked"
      ],
      "default": "Active",
      "required": true
    },
    "is_deleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "phone_number": {
      "type": "string",
      "required": true
    },
    "amenities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::amnesty.amnesty"
    }
  }
}
