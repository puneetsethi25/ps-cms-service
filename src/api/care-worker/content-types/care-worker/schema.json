{
  "kind": "collectionType",
  "collectionName": "care_workers",
  "info": {
    "singularName": "care-worker",
    "pluralName": "care-workers",
    "displayName": "care_worker",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "careHome": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::care-home.care-home"
    },
    "care_manger": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::care-manger.care-manger"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phoneNumber": {
      "type": "string"
    },
    "role": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive",
        "Blocked"
      ],
      "default": "Active",
      "required": true
    },
    "isDeleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other"
      ]
    },
    "languages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::language.language"
    },
    "description": {
      "type": "text"
    },
    "medicalSpecializations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medical-specialization.medical-specialization"
    },
    "image": {
      "type": "string"
    }
  }
}
