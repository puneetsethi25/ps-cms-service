    "question_catogery": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::question-catogery.question-catogery"
    },
    "question_type": {
      "type": "enumeration",
      "enum": [
        "Plain",
        "Radio",
        "Checkbox",
